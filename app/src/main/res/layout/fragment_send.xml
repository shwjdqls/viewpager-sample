<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="내가 좋아요한 사람들"
                android:textSize="14sp"
                android:textColor="#293739"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/imageView_delete1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_24_px_trash"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerview1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView1" />

            <TextView
                android:id="@+id/textview_list1"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text="목록 더 보기"
                android:textColor="#c8c8c8"
                android:textSize="10sp"
                android:background="@drawable/rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/recyclerview1"
                tools:ignore="HardcodedText,SmallSp" />

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="100dp"
                android:src="@drawable/icon_24_px_down"
                app:layout_constraintStart_toStartOf="@id/textview_list1"
                app:layout_constraintEnd_toEndOf="@id/textview_list1"
                app:layout_constraintTop_toTopOf="@id/textview_list1"
                app:layout_constraintBottom_toBottomOf="@id/textview_list1"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:text="내가 높은 점수를 준 사람들"
                android:textSize="14sp"
                android:textColor="#293739"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textview_list1"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/imageView_delete2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_24_px_trash"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/textView2"
                tools:ignore="ContentDescription" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerview2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView2" />

            <TextView
                android:id="@+id/textview_list2"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text="목록 더 보기"
                android:textColor="#c8c8c8"
                android:textSize="10sp"
                android:background="@drawable/rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/recyclerview2"
                tools:ignore="HardcodedText,SmallSp" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="100dp"
                android:src="@drawable/icon_24_px_down"
                app:layout_constraintStart_toStartOf="@id/textview_list2"
                app:layout_constraintEnd_toEndOf="@id/textview_list2"
                app:layout_constraintTop_toTopOf="@id/textview_list2"
                app:layout_constraintBottom_toBottomOf="@id/textview_list2"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:text="보낸 프로필 교환 목록"
                android:textSize="14sp"
                android:textColor="#293739"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textview_list2"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/imageView_delete3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_24_px_trash"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/textView3"
                tools:ignore="ContentDescription" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerview3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView3" />

            <TextView
                android:id="@+id/textview_list3"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:gravity="center"
                android:text="목록 더 보기"
                android:textColor="#c8c8c8"
                android:textSize="10sp"
                android:background="@drawable/rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/recyclerview3"
                tools:ignore="HardcodedText,SmallSp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="100dp"
                android:src="@drawable/icon_24_px_down"
                app:layout_constraintStart_toStartOf="@id/textview_list3"
                app:layout_constraintEnd_toEndOf="@id/textview_list3"
                app:layout_constraintTop_toTopOf="@id/textview_list3"
                app:layout_constraintBottom_toBottomOf="@id/textview_list3"
                tools:ignore="ContentDescription" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>